{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "semi": ["warn", "always"],
        "quotes": ["warn", "double"],
        "indent": ["warn", 4],
        "no-var": "error",
        "no-console": "warn",
        "comma-dangle": "off",
        "max-len": "off"
    }
}
